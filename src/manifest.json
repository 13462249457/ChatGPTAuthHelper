{
  "manifest_version": 2,
  "name": "ChatGPT Auth Helper",
  "version": "1.0.0",
  "description": "A simple Chrome extension to help you login ChatGPT.",
  "author": "Neo Peng",
  "homepage_url": "https://github.com/pengzhile/ChromeHttpRequestBlocker",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://chat.openai.com/*"
  ]
}
